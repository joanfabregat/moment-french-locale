{
  "name": "moment-french-locale",
  "version": "1.2.5",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/moment-french-locale.js",
  "module": "dist/moment-french-locale.mjs",
  "types": "dist/moment-french-locale.d.ts",
  "source": "src/moment-french-locale.ts",
  "exports": {
    "types": "./dist/moment-french-locale.d.ts",
    "import": "./dist/moment-french-locale.mjs",
    "require": "./dist/moment-french-locale.js",
    "module": "./dist/moment-french-locale.mjs",
    "default": "./dist/moment-french-locale.mjs"
  },
  "description": "Provides moment.js with French locale",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joanfabregat/moment-french-locale.git"
  },
  "dependencies": {
    "moment": "^2"
  },
  "author": {
    "name": "Joan Fabr√©gat",
    "email": "joan@codeinc.co"
  },
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
